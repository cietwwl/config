[user]
	email = zhengkai@gmail.com
	name = Zheng Kai

[alias]
	st = status -s -u
	co = checkout
	df = diff --no-ext-diff
	dl = diff --no-ext-diff HEAD~1 HEAD
	ci = commit
	br = branch
	po = push origin
	ps = push
	pl = pull
	l  = log --pretty=oneline -n 20 --graph --abbrev-commit
	ll = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --

[color]
	ui = true

[push]
	default = current

[diff]
    # external = ~/conf/script/git_diff_vim.sh
	algorithm = patience

[pager]
	diff =

[tig]
	tab-size = 4

[merge]
	tool = vimdiff

[color "status"]
	added = bold 121
	changed = bold 222
	untracked = bold 141

[core]
	safecrlf = true

[http]
